{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    "@babel/preset-react",
    ["@babel/preset-typescript", { "isTSX": true, "allExtensions": true }]
  ],
  "plugins": [
    [
      "babel-plugin-add-import-extension",
      {
        "extension": "mjs"
      }
    ]
  ]
}
