{
  "name": "@twist-ui/icons",
  "private": true,
  "author": "razzh <razzhavenir@163.com>",
  "license": "MIT",
  "scripts": {
    "g": "tsx ./scripts/generate.ts",
    "genIcon:vue": "pnpm run g --vue",
    "genIcon:vue-next": "pnpm run g --vue-next",
    "genIcon:react": "pnpm run g --react",
    "genIcon:all": "pnpm run clean && pnpm run g --vue --vue-next --react",
    "release:vue": "pnpm run clean && pnpm run g --vue && tsx ./scripts/release.ts --vue",
    "release:vue-next": "pnpm run clean && pnpm run g --vue-next && tsx ./scripts/release.ts --vue-next",
    "release:react": "pnpm run clean && pnpm run g --react && tsx ./scripts/release.ts --react",
    "release:all": "pnpm run clean && pnpm run genIcon:all && tsx ./scripts/release.ts --vue --vue-next --react",
    "release:no-build": "tsx ./scripts/release.ts --vue --vue-next --react",
    "format:snapshots": "prettier --write snapshots/**/*.ts",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@iconify/json": "^2.2.136",
    "@iconify/types": "^2.0.0",
    "@types/babel-core": "^6.25.10",
    "@types/babel__preset-env": "^7.9.5",
    "@types/fs-extra": "^11.0.3",
    "@types/gulp": "^4.0.16",
    "@types/lodash.camelcase": "^4.3.8",
    "@types/lodash.template": "^4.5.2",
    "@types/lodash.upperfirst": "^4.3.8",
    "@types/minimist": "^1.2.5",
    "@types/prompts": "^2.4.8",
    "@types/ramda": "^0.29.7",
    "@types/through2": "^2.0.40",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/babel-preset-jsx": "^1.4.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "kolorist": "^1.8.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.template": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "minimist": "^1.2.8",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "through2": "^4.0.2",
    "vinyl": "^3.0.0",
    "vue": "^3.3.8"
  }
}